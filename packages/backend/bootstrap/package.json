{
  "name": "cqrs-es-example-js-bootstrap",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:write-api": "ts-node src/index.ts writeApi",
    "start:read-api": "ts-node src/index.ts readApi",
    "start:local-rmu": "ts-node src/index.ts localRmu",
    "start:all": "concurrently -n \"writeApi,readApi,localRmu\" -c \"red,blue,green\" \"pnpm run start:write-api\" \"pnpm run start:read-api\" \"pnpm run start:local-rmu\"",
    "docker:build-backend": "tsc && rm -f dist/internal/*.test.d.ts && rm -f dist/internal/*.test.js && rm -fr dist/internal/test",
    "test": "jest --no-cache",
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "fix:eslint": "eslint --fix .",
    "fix:prettier": "prettier --write \"**/*.{js,ts,jsx,tsx,json}\"",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check \"**/*.{js,ts,jsx,tsx,json}\"",
    "clean": "rimraf ./dist",
    "clean-package": "clean-package"
  },
  "keywords": [],
  "author": "Junichi Kato <j5ik2o@gmail.com> (https://j5ik2o.me/)",
  "license": "MIT",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.134",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@types/winston": "^2.4.4",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "clean-package": "^2.2.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.47.0",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@aws-sdk/client-dynamodb": "^3.413.0",
    "@aws-sdk/client-dynamodb-streams": "^3.413.0",
    "@prisma/client": "^5.10.2",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^2.6.11",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1459.0",
    "cqrs-es-example-js-command-domain": "workspace:*",
    "cqrs-es-example-js-command-interface-adaptor-impl": "workspace:*",
    "cqrs-es-example-js-command-processor": "workspace:*",
    "cqrs-es-example-js-query-interface-adaptor": "workspace:*",
    "cqrs-es-example-js-rmu": "workspace:*",
    "event-store-adapter-js": "2.1.32",
    "fp-ts": "^2.16.2",
    "moment": "^2.29.4",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "reflect-metadata": "^0.2.1",
    "tslog": "^4.9.2",
    "yargs": "^17.7.2"
  },
  "engines": {
    "node": ">=20.5.0",
    "pnpm": ">=8",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  },
  "packageManager": "pnpm@8.15.8",
  "clean-package": {
    "replace": {
      "main": "dist/index.js",
      "types": "dist/index.d.ts"
    }
  }
}
